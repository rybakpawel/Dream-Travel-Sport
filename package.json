{
  "name": "dream-travel-sports",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 5173",
    "dev:server": "tsx watch server/src/index.ts",
    "build:server": "tsc -p server/tsconfig.json",
    "start:server": "node server/dist/index.js",
    "db:up": "docker compose -f server/docker-compose.yml up -d",
    "db:down": "docker compose -f server/docker-compose.yml down",
    "prisma:generate": "dotenv -e server/.env -- prisma generate --schema=server/prisma/schema.prisma",
    "prisma:migrate": "dotenv -e server/.env -- prisma migrate dev --schema=server/prisma/schema.prisma",
    "prisma:migrate:deploy": "dotenv -e server/.env -- prisma migrate deploy --schema=server/prisma/schema.prisma",
    "prisma:studio": "dotenv -e server/.env -- prisma studio --schema=server/prisma/schema.prisma",
    "delete:trips": "dotenv -e server/.env -- tsx server/scripts/delete-all-trips.ts",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "@types/pdfkit": "^0.17.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pdfkit": "^0.17.2",
    "resend": "^4.0.0",
    "zod": "^3.24.1"
  },
  "prisma": {
    "seed": "tsx server/prisma/seed.ts"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.2",
    "@types/node-cron": "^3.0.11",
    "dotenv-cli": "^7.4.2",
    "prettier": "^3.3.3",
    "prisma": "^6.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.11"
  }
}
